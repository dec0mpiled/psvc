    		var image748x60_localizedURLToCheck = "https://secure.cdn.us.playstation.com/psn/account/PSN/ForgotPasswordPage/no-no/Marketing_748x60.jpg";
    		var image569x58_localizedURLToCheck = "https://secure.cdn.us.playstation.com/psn/account/PSN/ForgotPasswordPage/no-no/Marketing_569x58.jpg";
